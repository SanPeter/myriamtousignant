---
layout: 'layout'
---
{% block content %}
<body class="single single-post single-format-standard bg_none ">
    {% include ('_partial/branding.twig') %}
    <div id="container" class="clearfix">
        {% include ('_partial/_header.twig') %}
        <div id="main" class="clearfix">
            <div id="pageHead">
                <h1>{{ page.title }}</h1>
            </div>
            <div id="content">
                {% include ('_partial/_social-icons.twig') %}
                {{ page.content }}
            </div>
        </div>    
        {% include ('_partial/_footer.twig') %}
    </div><!-- end container -->
    {% include ('_partial/_after-footer.twig') %}
</body>
{% endblock %}