<body class="page page-id-630 page-template page-template-portfolio-php bg_none ">
	<div id="container" class="clearfix">
		<%- partial('_partial/branding.ejs') %>
		<%- partial('_partial/header.ejs') %>
		<div id="main" class="clearfix">
			<div id="pageHead">
				<h1>Livres d'artiste</h1>
			</div>
			<div id="content" class="clearfix page page-id-630 page-template page-template-portfolio-php bg_none">
				<div id="projects">
					<div class="thumbs masonry">
						<% for(livre in  site.data.livres) { %>
						<div class="project small">
							<a href="<%= site.data.livres[livre].url %>" rel="bookmark">
								<img width="220" height="170" src="<%= site.data.livres[livre].image %>" class="thumb wp-post-image" alt="<%= site.data.livres[livre].titre %>" title="<%= site.data.livres[livre].titre %>" />
								<span class="title"><span><%= site.data.livres[livre].titre %></span></span>
							</a>
						</div>
						<% } %>
					</div>
				</div>
			</div>
		</div>
		<%- partial('_partial/footer') %>
	</div><!-- end container -->
	<%- partial('_partial/after-footer') %>
</body>

